    "jsonpointer": "^4.1.0",
    "jss": "^10.4.0",
    "jss-preset-default": "^10.4.0",
    "lodash.topath": "^4.5.2",
    "lodash.union": "^4.6.0",
    "vue": "^3.2.13",
    "vue-jss": "^0.0.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
