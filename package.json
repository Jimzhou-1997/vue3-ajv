{
  "name": "vue3-json-schema-form",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:core": "set TYPE=lib && vue-cli-service build --target lib --no-clean --name index lib/index.ts",
    "build:theme": "set TYPE=lib && vue-cli-service build --target lib --no-clean --name theme-default/index lib/theme-default/index.tsx",
    "build": "rimraf dist && npm run build:core && npm run build:theme",
    "test:unit": "vue-cli-service test:unit",
    "test:cov": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint --fix",
    "prettier": "prettier --write ."
